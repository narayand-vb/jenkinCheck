pipeline {
    agent any

    environment{
        CI = true
    }
    stages {
        stage('build') {
            steps {
                sh "docker build "
            }
        }

        stage('test') {
            steps {
                sh 'test successful'
            }
        }
        stage('Deploy') {
            steps {
                sh 'docker run -d -p80:3000 reactfirst'
            }
        }
        
    }
}