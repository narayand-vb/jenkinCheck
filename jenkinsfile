pipeline {
    agent any

    environment{
        CI = true
    }
    stages {
        stage('build') {
            steps {
                sh "docker compose up"
            }
        }

        stage('test') {
            steps {
                echo 'test successful'
            }
        }
        stage('Deploy') {
            steps {
                echo "Deployed"
            }
        }
        
    }
}